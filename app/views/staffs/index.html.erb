<%= render "shared/page_title", title: "Staff Index" %>

<div class="row">
  <div class="col-md-4 col-md-offset-4 center">
    <%= will_paginate %>
  </div>
</div>

<% @staffs.each do |staff| %>
  <div class="row">
    <div class="col-md-4 col-md-offset-4 left">
      <p>
        <h5><strong><%= staff.staff_name %></strong>, contact at <%= staff.staff_email %></h5>
      </p>
      <%= link_to "Go to Staff Profile", staff_path(staff) %>
    </div>
  </div>
<% end %>